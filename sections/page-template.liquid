
{{ 'sections.page-template.css' | asset_url | stylesheet_tag }}
<div class="c-page">
  <div class="c-page__header">
    <div class="c-page__header-inner">
      <h1 class="c-page__header-heading">{{page.title}}</h1>
    </div>
  </div>
  <div class="c-page__inner">
    {{page.content | replace: '<h1', '<h2' | replace: '</h1>', '</h2>'}}
  </div>
</div>
