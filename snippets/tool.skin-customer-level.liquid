{%- liquid
  
  assign customerLevel = 0

  for tag in customer.tags
    if tag contains 'week' 
      unless tag contains 'Mechanic'
        assign customerLevel = tag | split: '-' | last | plus: 0
      endunless
    endif
  endfor
-%}
